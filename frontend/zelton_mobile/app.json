{
  "expo": {
    "name": "ZeltonLivings",
    "slug": "zeltonlivings_mobile",
    "version": "1.0.0",
    "description": "Property management app for owners and tenants",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.zeltonlivings.mobile",
      "config": {
        "compileSdkVersion": 34,
        "targetSdkVersion": 34,
        "minSdkVersion": 21
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "zeltonlivings",
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "plugins": [
      "expo-font"
    ],
    "owner": "testingz6"
  }
}
