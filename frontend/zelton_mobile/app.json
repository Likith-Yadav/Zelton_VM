{
  "expo": {
    "name": "ZeltonLivings",
    "slug": "zeltonlivings_mobile",
    "version": "1.0.1",
    "description": "Property management app for owners and tenants",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "zeltonlivings",
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "986403db-373a-4c03-af68-7c98fc067a9d"
      }
    },
    "android": {
      "package": "com.zeltonlivings.mobile",
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "versionCode": 2,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "privacyPolicy": "https://zelton.in/privacy-policy"
    },
    "owner": ""
  }
}
